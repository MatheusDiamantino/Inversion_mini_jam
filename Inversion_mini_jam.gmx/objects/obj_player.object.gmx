          <argument>
            <kind>1</kind>
            <string>/// Follow Mouse

u = 1;
d = 1;
l = 1;
r = 1;


if(mouse_check_button_pressed(mb_left))
{
    
    if(!mouse_click){
        mouseX = mouse_x;
        mouseY = mouse_y;
    }
    
    tempx = sign(x - mouseX);
    tempy = sign(y - mouseY);
    
    if(state_player == STATE_GRAV_UP) u = 0;
    else if(state_player == STATE_GRAV_DOWN) d = 0;
    else if(state_player == STATE_GRAV_LEFT) l = 0;
    else if(state_player == STATE_GRAV_RIGHT) r = 0;
    
    if (place_meeting(mouseX, mouseY, obj_grav_box_up) &amp;&amp; state_player != STATE_GRAV_UP)
    {
        state_player = STATE_GRAV_UP;
        if(tempy &gt;= 0) mouse_click = true;
    } else
    if (place_meeting(mouseX, mouseY, obj_grav_box_down) &amp;&amp; state_player != STATE_GRAV_DOWN)
    {
        state_player = STATE_GRAV_DOWN;
        if(tempy &lt; 0) mouse_click = true;
    } else   
    if (place_meeting(mouseX, mouseY, obj_grav_box_left) &amp;&amp; state_player != STATE_GRAV_LEFT)
    {
        state_player = STATE_GRAV_LEFT;
       if(tempx &gt;= 0) mouse_click = true;
    } else   
    if (place_meeting(mouseX, mouseY, obj_grav_box_right) &amp;&amp; state_player != STATE_GRAV_RIGHT)
    {
        state_player = STATE_GRAV_RIGHT;
        if(tempx &lt; 0) mouse_click = true;
    }
}

if(mouse_click){
    dist = point_distance(mouseX, mouseY, x, y);
    dir = point_direction(x, y, mouseX, mouseY);
    
    if(dist &lt; 10 || d*place_meeting(x, y + 1, obj_wall_parent)
        || u*place_meeting(x, y - 1, obj_wall_parent)
        || r*place_meeting(x + 1, y, obj_wall_parent)
        || l*place_meeting(x - 1, y, obj_wall_parent))
    {
        mouse_click = false;
    } else
    {
        x += lengthdir_x(2, dir);
        y += lengthdir_y(2, dir);
    }
    
    
}

